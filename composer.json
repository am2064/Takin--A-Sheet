{
	"name": "am2064/Takin--A-Sheet",
	"description": "Rapid Character Sheet Generator",
	"keywords": ["character sheets","character generator","tabletop games"],
	"license": "MIT",
	"require": {
		"laravel/framework": "dev-master",
		"binarix/foundation-pagination": "dev-master",
		"stevenmaguire/zurb-foundation-laravel": "dev-master",
		"way/generators": "dev-master",
		"doctrine/dbal": "dev-master"
	},
	"autoload": {
		"classmap": [
			"app/commands",
		"app/controllers",
		"app/models",
		"app/database/migrations",
		"app/database/seeds",
		"app/tests/TestCase.php"
			]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
		"php artisan optimize"
			],
		"post-update-cmd": [
			"php artisan clear-compiled",
		"php artisan optimize"
			],
		"post-create-project-cmd": [
			"php artisan key:generate"
			]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
